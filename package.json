    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "rollup": "^3.29.4",
    "rollup-plugin-typescript2": "^0.35.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
